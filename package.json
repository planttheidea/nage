{
  "author": "tony.quetano@planttheidea.com",
  "browser": "dist/nage.js",
  "bugs": {
    "url": "https://github.com/planttheidea/nage/issues"
  },
  "description": "Efficient, tiny object pool",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.18",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "in-publish": "^2.0.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^24.8.0",
    "nyc": "^14.1.1",
    "prop-types": "^15.7.2",
    "rollup": "^1.12.3",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "homepage": "https://github.com/planttheidea/nage#readme",
  "keywords": [
    "object",
    "pool",
    "memory"
  ],
  "license": "MIT",
  "main": "dist/nage.cjs.js",
  "module": "dist/nage.esm.js",
  "name": "nage",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/nage.git"
  },
  "scripts": {
    "benchmark": "npm run dist -- --no-comments && node benchmark/index.js",
    "build": "NODE_ENV=production rollup -c",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development webpack-dev-server --colors --progress --config=webpack/webpack.config.js",
    "dist": "npm run clean && npm run build",
    "lint": "NODE_ENV=test eslint src/*.ts",
    "lint:fix": "NODE_ENV=test eslint src --fix",
    "prepublish": "if in-publish; then npm run prepublish:compile; fi",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run dist",
    "release": "release-it",
    "release:beta": "release-it --config=.release-it.beta.json",
    "release:dry": "release-it --dry-run",
    "start": "npm run dev",
    "test": "NODE_PATH=. jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "sideEffects": false,
  "types": "index.d.ts",
  "version": "1.3.0"
}
